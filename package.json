{
  "name": "vscode-gpm",
  "displayName": "gpm",
  "description": "vscode extension for gpm",
  "version": "0.0.1",
  "publisher": "gpm",
  "engines": {
    "vscode": "^1.21.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension",
  "activationEvents": [
    "onView:GPM",
    "onCommand:gpm.addProject"
  ],
  "contributes": {
    "views": {
      "explorer": [
        {
          "id": "GPM",
          "name": "GPM"
        }
      ]
    },
    "menus": {
      "view/item/context": [
        {
          "group": "context@1",
          "command": "gpm.openProject"
        }
      ]
    },
    "commands": [
      {
        "command": "gpm.addProject",
        "title": "GPM: Add Project"
      },
      {
        "command": "gpm.openProject",
        "title": "GPM: Open Project"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/fs-extra": "^5.0.1",
    "@types/mocha": "^2.2.42",
    "@types/node": "^9.4.7",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1",
    "vscode": "^1.1.6"
  },
  "dependencies": {
    "fs-extra": "^5.0.0",
    "which": "^1.3.0"
  }
}
